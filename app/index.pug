block dev
	+connect-script('js/lib/instafeed.min.js')
	.vmodal__content#question
		+b.FORM.vform._block
			+e.content
				+e.row._line
					+b.INPUT.vform-control(type="text" placeholder="Имя*" name="name")
					+e.SPAN.line
				+e.row._line
					+b.INPUT.vform-control(type="text" placeholder="E-mail*" name="email")
					+e.SPAN.line
				+e.row._line
					+b.INPUT.vform-control(type="tel" placeholder="Телефон*" name="phone")
					+e.SPAN.line
				+e.row._line
					+b.TEXTAREA.vform-control(type="text" placeholder="Запитання*" name="message" rows="5")
				+e.row._btn._line
					+b.BUTTON.vbtn._block._large._brand-1.effect._bounce-top(type="submit") Задати запитання
